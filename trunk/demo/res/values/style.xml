<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="mytheme" parent="@android:style/Theme.Light">
		<item name="android:windowNoTitle">true</item>
	</style>


	<style name="PopupAnimation" parent="android:Animation">
		<item name="android:windowEnterAnimation">@anim/popup_enter</item>
		<item name="android:windowExitAnimation">@anim/popup_exit</item>
	</style>
		<style name="PopupAnimation1" parent="android:Animation">
		<item name="android:windowEnterAnimation">@anim/popup_enter1</item>
		<item name="android:windowExitAnimation">@anim/popup_exit1</item>
	</style>
	
	<style name="PopupMenuAnimation" parent="android:Animation">
		<item name="android:windowEnterAnimation">@anim/popup_menu_in</item>
		<item name="android:windowExitAnimation">@anim/popup_menu_out</item>
	</style>

	<style name="titleTextStyle">
		<item name="android:textColor">#000000</item>
		<item name="android:textSize">@dimen/titleTextStyle_size</item>
	</style>

	<style name="titleStyle">
		<item name="android:background">@drawable/head_bg</item>
		<item name="android:layout_height">@dimen/title_background_height</item>
		<item name="android:layout_width">fill_parent</item>
	</style>

	<style name="trun">
		<item name="android:layout_width">@dimen/trun_width</item>
		<item name="android:layout_height">@dimen/trun_height</item>
		<item name="android:layout_marginLeft">@dimen/trun_marginLeft</item>
		<item name="android:layout_marginRight">@dimen/trun_marginRight</item>
		<item name="android:background">@drawable/dot_white</item>
		<item name="android:visibility">gone</item>
		<item name="android:layout_gravity">right|center_vertical</item>
	</style>


	<style name="sys_setting_font_black">
		<item name="android:textColor">#000000</item>
		<item name="android:textSize">@dimen/sys_setting_font_black_size</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_alignParentTop">true</item>
		<item name="android:layout_alignParentLeft">true</item>
	</style>


	<style name="sys_setting_font_grary">
		<item name="android:textColor">#808080</item>
		<item name="android:textSize">@dimen/sys_setting_font_grary_size</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
	</style>

	<style name="sys_setting_btn">
		<item name="android:layout_marginRight">@dimen/sys_setting_btn_marginRight</item>
		<item name="android:layout_centerInParent">true</item>
		<item name="android:layout_alignParentRight">true</item>
		<item name="android:layout_width">@dimen/sys_setting_btn_width</item>
		<item name="android:layout_height">@dimen/sys_setting_btn_height</item>
	</style>

<!--	<style name="splash_diolag_loading">-->
<!--		<item name="android:indeterminateDrawable">@drawable/splash_diolag_loading</item>-->
<!--	</style>-->


    <style name="transparentdialog">
      <item name="android:windowBackground">@android:color/transparent</item>
      <item name="android:windowFrame">@null</item>
      <item name="android:windowContentOverlay">@null</item>
      <item name="android:windowIsFloating">true</item>
      <item name="android:windowIsTranslucent">true</item>
      <item name="android:windowNoTitle">true</item>
      <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
      <item name="android:backgroundDimEnabled">false</item>
     </style> 

  <style name="bookmark_text">
    <item name="android:textSize">@dimen/bookmark_text_size</item>
    <item name="android:textColor">#000000</item>
    <item name="android:layout_width">70px</item>
    <item name="android:layout_height">@dimen/bookmark_text_height</item>
    </style>


    <style name="Theme" parent="android:Theme">
        <item name="android:windowAnimationStyle">@style/Animation</item>
    </style>
 

    <style name="Animation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@anim/popup_exit</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@anim/popup_exit</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@anim/popup_exit</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@anim/popup_exit</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@anim/popup_exit</item>
       
    </style>

<style name="bookMarkAnimation" >    

             <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@anim/left_out</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@anim/left_out</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@anim/left_out</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@anim/left_out</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@anim/left_out</item>


</style>   


